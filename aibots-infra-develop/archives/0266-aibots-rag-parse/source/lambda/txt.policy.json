{
  "Version" : "2012-10-17",
  "Statement" : [
    {
      "Effect" : "Allow",
      "Action" : [
        "s3:ListBucket",
				"s3:PutObject",
        "sqs:ChangeMessageVisibility",
        "sqs:DeleteMessage",
        "sqs:GetQueueAttributes",
        "sqs:ReceiveMessage"
      ],
      "Resource" : [
        "${PROJECT__BUCKET_ARN}",
        "${PROJECT__BUCKET_ARN}/*",
        "${ANALYTICS__BUCKET_ARN}",
        "${ANALYTICS__BUCKET_ARN}/*",
        "${SOURCE__SQS_ARN}"
      ]
    },
    {
      "Effect" : "Allow",
      "Action" : [
        "sqs:sendmessage"
      ],
      "Resource" : [
        "${PROJECT_RAG_STATUS_MAIN__SQS_ARN}",
        "${PROJECT_RAG_CHUNK_TEXT_SEMANTIC__SQS_ARN}",
        "${PROJECT_RAG_CHUNK_TEXT_FIXED__SQS_ARN}",
        "${PROJECT_RAG_CHUNK_TEXT_ANCHORS__SQS_ARN}",
        "${PROJECT_RAG_CHUNK_TEXT_DATAFRAME__SQS_ARN}"
      ]
    },
    {
      "Effect" : "Allow",
      "Action" : [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource" : [
        "${PROJECT__BUCKET_ARN}",
        "${PROJECT__BUCKET_ARN}/*"
      ]
    }
  ]
}